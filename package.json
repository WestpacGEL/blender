{
	"name": "blender",
	"description": "The blender for the Westpac Design System",
	"repository": "https://github.com/WestpacGEL/blender",
	"author": "Westpac GEL Team <gel@westpac.com.au>",
	"contributors": [
		{
			"name": "Dominik Wilkowski",
			"email": "Hi@Dominik-Wilkowski.com",
			"url": "https://dominik-wilkowski.com"
		},
		{
			"name": "Thomas Walker",
			"email": "bladey@me.com",
			"url": "https://tomwalkergelato.com"
		}
	],
	"license": "GPL-3.0+",
	"private": true,
	"scripts": {
		"start": "echo 'TODO'",
		"nuke": "rm -rf node_modules && rm yarn.lock",
		"fresh": "yarn nuke && yarn",
		"build": "npx mkdirp lib && babel src --out-dir lib",
		"format": "prettier --write \"**/*.{js,md,mdx,json}\"",
		"test:format": "prettier --list-different \"**/*.{js,md,mdx,json}\"",
		"test:unit": "jest",
		"test:unit-watch": "jest --watch --coverage",
		"test": "yarn test:format && yarn test:unit"
	},
	"jest": {
		"displayName": "Blender",
		"testEnvironment": "node",
		"testRegex": "\\.spec.js$",
		"collectCoverageFrom": [
			"src/*.js"
		],
		"coverageThreshold": {
			"global": {
				"branches": 95,
				"functions": 100,
				"lines": 95,
				"statements": 95
			}
		}
	},
	"dependencies": {
		"@emotion/core": "^10.0.28",
		"@emotion/weak-memoize": "^0.2.5",
		"cfonts": "^2.8.1",
		"create-emotion-server": "^10.0.27",
		"react": "^16.13.0",
		"react-dom": "^16.13.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.1",
		"jest-cli": "^25.1.0",
		"prettier": "^1.19.1"
	}
}
